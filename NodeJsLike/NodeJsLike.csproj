<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Description>
      Helps host web apps on console based apps with node js syntax
    </Description>
    <PackageId>NodeJsLike</PackageId>
    <PackageProjectUrl>https://github.com/neville-nazerane/NetCore.NodeJsLike</PackageProjectUrl>
    <PackageTags>nodejs node-js webapp</PackageTags>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.AspNetCore" Version="2.1.3" />
    <PackageReference Include="Microsoft.AspNetCore.Hosting" Version="2.1.1" />
    <PackageReference Include="Microsoft.AspNetCore.Http" Version="2.1.1" />
  </ItemGroup>

</Project>
